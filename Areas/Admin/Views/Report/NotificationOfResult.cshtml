<script src="~/Scripts/jquery.min.js"></script>
<script src="~/Scripts/responsive-tabs.js"></script>

<script type="text/javascript">
    function resizeIframe(obj) {
        obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
    }
</script>

<div style="color: black">
    <h3>
        Notification of Result
    </h3>
</div>

<div class="alert alert-dismissible alert-success">
    <p>
        <b>Notification of Result</b> shows a notification of the final examination result.
    </p>
</div>

<iframe src="~/Reports/Presenter/Result/NotificationOfResult.aspx?personId=@ViewBag.personId&semesterId=@ViewBag.semesterId&sessionId=@ViewBag.sessionId&programmeId=@ViewBag.programmeId&departmentId=@ViewBag.departmentId&levelId=@ViewBag.levelId "  style="width: 100%;" frameborder="0" scrolling="no" id="iframe" onload=' javascript:resizeIframe(this); '></iframe>

